{
  "configurations": [
    {
      "name": "Node Remote Debug",
      "type": "node",
      "port": 9229,
      "restart": true,
      "request": "attach",
      "protocol": "inspector",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "localRoot": "${workspaceFolder}/src",
      "remoteRoot": "/app/src"
    }
  ]
}
